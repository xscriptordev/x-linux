{
    "app_config": null,
    "archinstall-language": "English",
    "auth_config": {},
    "bootloader": "Grub",
    "custom_commands": [
        "printf 'NAME=\"X\"\\nPRETTY_NAME=\"X\"\\nID=xos\\nID_LIKE=arch\\nBUILD_ID=rolling\\nANSI_COLOR=\"0;36\"\\nHOME_URL=\"https://dev.xscriptor.com/xos\"\\nDOCUMENTATION_URL=\"https://dev.xscriptor.com/xos/docs\"\\nSUPPORT_URL=\"https://dev.xscriptor.com/xos/support\"\\nBUG_REPORT_URL=\"https://github.com/xscriptordev/xos\"\\nLOGO=xos\\n' > /etc/os-release",
        "sed -i 's/^GRUB_DISTRIBUTOR=.*/GRUB_DISTRIBUTOR=\"X\"/' /etc/default/grub || echo 'GRUB_DISTRIBUTOR=\"X\"' >> /etc/default/grub",
        "for f in /boot/loader/entries/*.conf /efi/loader/entries/*.conf; do [ -f \"$f\" ] && sed -i 's/Arch Linux/X/g' \"$f\"; done || true",
        "grub-mkconfig -o /boot/grub/grub.cfg || true"
    ],
    "disk_config": {
        "config_type": "pre_mounted_config",
        "mountpoint": "/mnt/archinstall"
    },
    "hostname": "x",
    "kernels": [
        "linux"
    ],
    "locale_config": {
        "kb_layout": "es",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US.UTF-8"
    },
    "mirror_config": {
        "custom_repositories": [],
        "custom_servers": [],
        "mirror_regions": {
            "Spain": [
                "https://ftp.rediris.es/mirror/archlinux/$repo/os/$arch",
                "http://es.mirrors.cicku.me/archlinux/$repo/os/$arch",
                "https://es.mirrors.cicku.me/archlinux/$repo/os/$arch",
                "https://nox.panibrez.com/archlinux/$repo/os/$arch",
                "http://mirror.raiolanetworks.com/archlinux/$repo/os/$arch",
                "https://mirror.raiolanetworks.com/archlinux/$repo/os/$arch",
                "http://mirror.es.cdn-perfprod.com/archlinux/$repo/os/$arch",
                "https://mirror.es.cdn-perfprod.com/archlinux/$repo/os/$arch"
            ]
        },
        "optional_repositories": []
    },
    "network_config": {
        "type": "nm"
    },
    "ntp": true,
    "packages": [],
    "parallel_downloads": 0,
    "profile_config": {
        "gfx_driver": "All open-source",
        "greeter": "gdm",
        "profile": {
            "custom_settings": {
                "GNOME": {}
            },
            "details": [
                "GNOME"
            ],
            "main": "Desktop"
        }
    },
    "script": null,
    "services": [],
    "swap": true,
    "timezone": "UTC",
    "uki": false,
    "version": "3.0.13"
}